{
  "manifest_version": 3,
  "name": "CounterAct Mitigation",
  "version": "1.1",
  "description": "Detect potential fake or clone applications with advanced verification",
  "permissions": [
      "activeTab", 
      "scripting", 
      "storage", 
      "webNavigation"
  ],
  "host_permissions": [
      "<all_urls>"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
      }
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png", 
      "128": "icon128.png"
  }
}